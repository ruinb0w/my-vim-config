{
  "coc": {
    "preferences": {
      "formatOnSaveFiletypes": [
        "rust",
        "scss",
        "javascript",
        "typescript",
        "html",
        "json",
        "markdown",
        "css",
        "vue",
        "lua"
      ]
    }
  },
  "prettier.disableLanguages": [],
  "prettier.printWidth": 100,
  "Lua.diagnostics": {
    "globals": ["vim"],
    "disable": ["lowercase-global"]
  }
}
